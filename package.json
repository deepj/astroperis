{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.2.1",
    "@hotwired/turbo-rails": "^7.2.4",
    "@rails/activestorage": "^7.0.4",
    "autoprefixer": "^10.4.13",
    "debounce": "^1.2.1",
    "esbuild": "^0.15.17",
    "flowbite": "^1.5.4",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4",
    "uppy": "^3.3.1"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --public-path=assets",
